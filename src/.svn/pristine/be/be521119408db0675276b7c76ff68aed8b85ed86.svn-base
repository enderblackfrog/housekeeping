@import 'base';
.p-center {
    height: 140px;
    width: 100%;
    text-align: center;
    border-bottom: 10px solid @housekeeping_body_color;
    background: #ffffff;
    .p-center-photos{
        width:35%;
        height:100px;
        float: left;
        margin: 20px 0;
        img{
            height: 100px;
            border-radius: 60px;
        }
    }
    .p-center-info{
        text-align: left;
        height:60px;
        float: left;
        width: 65%;
        margin: 40px 0;
        font-size: @housekeeping_font_size4;
        color: @housekeeping_font_color2;
        div{
            height: 18px;
            width:100%;
            margin-bottom: 2px;
        }
    }
}
.p-relevant{
    width: 96%;
    border-bottom: 10px solid @housekeeping_body_color;
    padding:0 2%;
    background: #ffffff;
    height: auto;
}
.div1{
    width: 100%;
    height: 40px;
    border-bottom: 1px solid @housekeeping_body_color;
    line-height: 40px;
    color: @housekeeping_font_color2;
    font-size:  @housekeeping_font_size4;
}
